<% if @coupon.errors.empty? %>
  $('#<%= "coupon_#{@coupon.id}" %>').replaceWith('<%= escape_javascript render(:partial => "/breeze/commerce/coupons/coupon", :object => @coupon) %>');
  $('#coupon-details').dialog('close');
<% else %>
  $('#coupon-details').html('<%= escape_javascript render("edit") %>');
<% end %>

