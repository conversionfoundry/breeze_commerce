- content_for :title, "Tags"
= pane_layout do |pane|
  = pane.inner do
    = scrollable_layout do
      .actions.horizontal
        = link_to "New Tag", new_admin_store_tag_path, :class => "large new green tag button"
      %h1 Tags
      .box
        = will_paginate @tags
        = render :partial => "breeze/admin/commerce/tags/tags", :locals => { :tags => @tags }
- content_for :left do
  = render :partial => "breeze/admin/commerce/shared/left"