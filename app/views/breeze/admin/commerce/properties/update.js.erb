<% if @property.errors.empty? %>
  $('#product-properties').html('<%= escape_javascript render(:partial => "breeze/admin/commerce/products/properties", :object => @product.properties) %>');
  $('#property-details').dialog('close');
  $('#product-variants').html('<%= escape_javascript render(:partial => "breeze/admin/commerce/products/variants", :object => @product.variants) %>');
<% else %>
  $('#property-details').html('<%= escape_javascript render(:file => "breeze/admin/commerce/properties/edit") %>');
  new $.TextboxList("#property_option_names");
<% end %>
