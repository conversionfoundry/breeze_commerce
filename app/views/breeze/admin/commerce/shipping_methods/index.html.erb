<% content_for :title, "shipping_methods" %>

<%= pane_layout do |panes| %>
  <%= panes.inner do %>
    <%= scrollable_layout do %>
      <h1>Shipping Methods <%= link_to "New shipping method", new_admin_store_shipping_method_path, :class => "large green button new shipping_method" %></h1>
      <%= will_paginate @shipping_methods %>      
      <%= render :partial => "breeze/admin/commerce/shipping_methods/shipping_methods", :object => @shipping_methods %>
    <% end %>
  <% end %>
<% end %>

<% content_for :left do %>
  <%= render :partial => "breeze/admin/commerce/commerce/left" %>
<% end %>

