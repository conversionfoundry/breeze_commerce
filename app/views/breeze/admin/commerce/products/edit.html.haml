- content_for :title, "Editing Product"
- content_for :left do
  = render :partial => "breeze/admin/commerce/shared/left"

= pane_layout do |main|
  - main.inner do
    = scrollable_layout do
      .product-edit.box{:id => "product_#{@product.id}"}
        = render "breeze/admin/commerce/products/form", :url => admin_store_product_path(@product)