<table class="variants">
  <thead>
    <tr>
      <th class="image first"></th>
      <th class="name" style="">Name</th>
      <th class="price">Price</th>
      <th class="actions last" style=""></th>
    </tr>
  </thead>
  <tbody>
    <% if variants.any? %>
      <%= render :partial => "/partials/commerce/variant", :collection => variants, :locals => {:variant_count => variants.count} %>
    <% else %>
      <tr><td class="empty" colspan="5">There are no variants available for this product.</td></tr>
    <% end %>
  </tbody>
  <tfoot>
    <tr>
    </tr>
  </tfoot>
</table>
